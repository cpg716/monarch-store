{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "app-commands",
    "notification:default",
    "aptabase:default",
    {
      "identifier": "fs:allow-read",
      "allow": [
        {
          "path": "$CACHE/monarch-store/**"
        }
      ]
    }
  ]
}